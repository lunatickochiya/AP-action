-+				  rtw8812a_cck_rx_pwr);
-+
-+	if (pkt_stat->rate >= DESC_RATE6M)
-+		return;
-+
-+	if (rtwdev->hal.cck_high_power)
-+		return;
-+
-+	if (pkt_stat->rssi >= 80)
-+		pkt_stat->rssi = ((pkt_stat->rssi - 80) << 1) +
-+				 ((pkt_stat->rssi - 80) >> 1) + 80;
-+	else if (pkt_stat->rssi <= 78 && pkt_stat->rssi >= 20)
-+		pkt_stat->rssi += 3;
