-+static int rtw88xxa_llt_init(struct rtw_dev *rtwdev, u32 boundary)
-+{
-+	u32 last_entry = 255;
-+	int status = 0;
-+	u32 i;
-+
-+	for (i = 0; i < boundary - 1; i++) {
-+		status = rtw88xxa_llt_write(rtwdev, i, i + 1);
-+		if (status)
-+			return status;
-+	}
-+
-+	status = rtw88xxa_llt_write(rtwdev, boundary - 1, 0xFF);
