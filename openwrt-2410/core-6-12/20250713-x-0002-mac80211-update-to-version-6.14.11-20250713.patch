-+				   u32 rf_num)
-+{
-+	u32 i;
-+
-+	/* [31] = 0 --> Page C */
-+	rtw_write32_mask(rtwdev, REG_CCASEL, BIT(31), 0x0);
-+
-+	/* Save RF Parameters */
-+	for (i = 0; i < rf_num; i++) {
-+		rfa_backup[i] = rtw_read_rf(rtwdev, RF_PATH_A,
-+					    backup_rf_reg[i], MASKDWORD);
