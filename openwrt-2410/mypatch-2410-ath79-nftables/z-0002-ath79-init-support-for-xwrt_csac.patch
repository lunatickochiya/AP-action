+/ {
+	model = "XWRT CSAC";
+	compatible = "xwrt,csac", "qca,qca9563";
+
+	aliases {
+		led-boot = &led_sys;
+		led-failsafe = &led_sys;
+		led-running = &led_sys;
+		led-upgrade = &led_sys;
