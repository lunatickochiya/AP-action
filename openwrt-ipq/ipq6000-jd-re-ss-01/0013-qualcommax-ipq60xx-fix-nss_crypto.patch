From f03b8019d7c5136999bb4ddc32de586ea2030bda Mon Sep 17 00:00:00 2001
From: Lienol <23146169+Lienol@users.noreply.github.com>
Date: Sat, 7 Jun 2025 21:11:05 +0800
Subject: [PATCH 13/13] qualcommax/ipq60xx: fix nss_crypto

---
 .../files/arch/arm64/boot/dts/qcom/ipq6018-nss.dtsi       | 8 ++++----
 1 file changed, 4 insertions(+), 4 deletions(-)

diff --git a/target/linux/qualcommax/files/arch/arm64/boot/dts/qcom/ipq6018-nss.dtsi b/target/linux/qualcommax/files/arch/arm64/boot/dts/qcom/ipq6018-nss.dtsi
index 97a7d3fd08..b28aa54c27 100644
--- a/target/linux/qualcommax/files/arch/arm64/boot/dts/qcom/ipq6018-nss.dtsi
+++ b/target/linux/qualcommax/files/arch/arm64/boot/dts/qcom/ipq6018-nss.dtsi
@@ -105,8 +105,8 @@
 
 	nss_crypto: qcom,nss_crypto {
 		compatible = "qcom,nss-crypto";
-		#address-cells = <1>;
-		#size-cells = <1>;
+		#address-cells = <2>;
+		#size-cells = <2>;
 		qcom,max-contexts = <64>;
 		qcom,max-context-size = <32>;
 		ranges;
@@ -114,7 +114,7 @@
 		eip197_node {
 			compatible = "qcom,eip197";
 			reg-names = "crypto_pbase";
-			reg = <0x39800000 0x7ffff>;
+			reg = <0x0 0x39800000 0x0 0x7ffff>;
 			clocks = <&gcc GCC_NSS_CRYPTO_CLK>,
 				<&gcc GCC_NSSNOC_CRYPTO_CLK>,
 				<&gcc GCC_CRYPTO_PPE_CLK>;
@@ -206,4 +206,4 @@
 		phy_access_mode = <0x00>;
 	};
 
-};
\ No newline at end of file
+};
